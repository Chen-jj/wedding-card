{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "SfaJS CLI Debugger",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceRoot}",
      "runtimeExecutable": "npm",
      "windows": {
        "runtimeExecutable": "npm.cmd"
      },
      "runtimeArgs": [
        "run",
        "dev"
      ],
      "env": {
        "NODE_ENV": "local"
      },
      "console": "integratedTerminal",
      "restart": true,
      "autoAttachChildProcesses": true
    }
  ]
}